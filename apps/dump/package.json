{
	"name": "@apps/dump",
	"version": "0.2.0",
	"scripts": {
		"dev": "vite dev --port 5006",
		"preview": "vite preview --port 5006",
		"build": "npm run pre:db && rimraf .netlify && vite build",
		"pre:db": "prisma db pull && prisma generate",
		"package": "svelte-kit package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@firebase/app-types": "0.x",
		"@playwright/test": "^1.22.2",
		"@prisma/client": "^4.5.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "1.0.0-next.78",
		"@sveltejs/kit": "^1.0.0-next.504",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"bcrypt": "^5.1.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"firebase": "^9.12.1",
		"firebase-admin": "^11.2.0",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"prisma": "^4.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.1.4",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.0",
		"dotenv": "^16.0.1",
		"cookie": "^0.4.2"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"daisyui": "^2.17.0",
		"material-icons": "^1.11.10"
	},
	"type": "module"
}
