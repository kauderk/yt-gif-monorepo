{
	"name": "@apps/dump",
	"description": "",
	"author": "YT GIF Client app",
	"version": "0.2.0",
	"inherits": [
		"@packages/dependencies/package.packages.json"
	],
	"scripts": {
		"dev": "vite dev --port 5006",
		"preview": "vite preview --port 5006",
		"build": "npm run pre:pckg && npm run pre:db && rimraf dist && vite build",
		"pre:pckg": "cd ../../packages/yt-gif && npm run sync",
		"pre:db": "prisma db pull && prisma generate",
		"package": "svelte-kit package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@firebase/app-types": "0.x",
		"@prisma/client": "^4.5.0",
		"@types/cookie": "^0.5.1",
		"bcrypt": "^5.1.0",
		"cookie": "^0.4.2",
		"firebase-admin": "^11.2.0",
		"prisma": "^4.5.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.4.13",
		"postcss": "^8.4.20",
		"tailwindcss": "^3.2.4",
		"vite": "4",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-netlify": "1.0.0-next.84",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"dotenv": "^16.0.3",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"firebase": "^9.15.0",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.2",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@packages/components": "workspace:^0.2.0",
		"@packages/config": "workspace:^0.2.0",
		"@packages/metadata": "workspace:^0.2.0",
		"@packages/yt-gif": "workspace:^0.2.0",
		"daisyui": "^2.17.0",
		"material-icons": "^1.11.10"
	},
	"type": "module"
}
